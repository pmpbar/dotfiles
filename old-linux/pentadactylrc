colorscheme solarized-dark
set complete=Ssh
set wildmode=list:longest,list:full

set hintkeys=hjklasdfgyuiopqwertnmzxcvb

imap -builtin jk <ESC>
nmap -builtin j 8j
nmap -builtin k 8k
nmap -builtin h 8h
nmap -builtin l 8l

nmap -builtin <Space>h :tabprevious<CR>
nmap -builtin <Space>l :tabnext<CR>


set guioptions-=s


